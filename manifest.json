{
  "name": "Gladex Extension",
  "version": "1.1",
  "description": "Gladex Extension with Dynamic Settings UI",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "settings.html",
    "default_title": "Gladex Settings"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["thirdparty/jquery.js", "content.js"],
      "run_at": "document_end"
    }
  ]
}